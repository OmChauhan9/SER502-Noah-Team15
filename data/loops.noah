for (int i = 0; i < 5; i = i + 1) {
    print("For loop iteration: " + i);
}

int count = 0;
while (count < 5) {
    print("While loop count: " + count);
    count = count + 1;
}

for (int i = 0; i < 3; i = i + 1) {
    int count1 = 0;
    while (count1 < 2) {
        print("While2 loop count: " + count1);
        count1 = count1 + 1;
    }
    print("For loop iteration: " + i);
}